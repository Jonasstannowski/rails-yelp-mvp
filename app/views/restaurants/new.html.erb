<%= form_for(@restaurant) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <br>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <br>
  <br>
  <%= f.label :phone_number %>
  <%= f.number_field :phone_number %>
  <br>
  <br>
  <%= f.label :category %>
  <%= f.number_field :category %>
  <br>
  <br>
  <%= f.submit %>
<% end %>
